/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.title{
  font-size: 16px;
  padding: 0 20rpx;
  margin:30rpx 0;
  border-left: 2px solid red;
}


/* 列表 */
.cell{
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #ddd;
  font-size:16px;
  background-color:#fff;
  margin: 0 10rpx;
}
.cell .cell-item{
  display: flex;
  flex-direction: column;
  padding:20rpx 20rpx;
  width:600rpx;
}
.cell-item .muisc-desc{
  font-size: 12px;
  color: #888;
}
.cell-item .muisc-name{
  font-size: 17px;
  color: #333;
}
.cell image{
  width: 50rpx;
  height: 50rpx;
  margin-right: 10rpx;
}

.ell{
  overflow:hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.new-list{
  clear:both;
}
.list-title{
  height:50rpx;
  width:100%;
  background-color: #eeeff0;
  font-size: 12px;
  line-height: 50rpx;
  padding-left: 20rpx;
  clear:both;
}
.list-desc{
  padding:20rpx 30rpx;
}
.list-desc .list-tag{
  margin-bottom: 20rpx;
}
.list-desc .list-tag .tag-item{
  padding:5rpx 15rpx;
  border: 1rpx rgba(0, 0, 0, 0.1) solid;
  border-radius: 25rpx;
  margin:0 10rpx;
}
